###
<h3>Team Stats</h3>
<table cellpadding="0" cellspacing="0" class="teamStatTable deluxe wide400">
<tbody><tr id="header-sub">
<th scope="col" abbr="">Total yards</th>
<th scope="col" abbr="">First downs</th>
<th scope="col" abbr="">Rushing yards</th>
<th scope="col" abbr="">Yds/Att</th>
<th scope="col" abbr="">Passing yards</th>
<th scope="col" abbr="">Yds/Catch</th>
</tr>
<tr class="resultsText">
<VAR $yardsAtt = round($TeamSeasonStats_RushingYardsPerAttempt,2)>
<VAR $yardsCatch = round($TeamSeasonStats_YardsPerCatch,2)>
<td align="right">{$TeamSeasonStats_TotalYards}</td>
<td align="right">{$TeamSeasonStats_FirstDowns}</td>
<td align="right">{$TeamSeasonStats_RushingYards}</td>
<td align="right">{$yardsAtt}</td>
<td align="right">{$TeamSeasonStats_PassingYards}</td>
<td align="right">{$yardsCatch}</td>
</td>
</tr>
</tbody>
</table>
