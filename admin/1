<script language="javascript" type="text/javascript">
var userPrivLevel = 9;
var pageRequiredPrivLevel = 4;
var startInEditMode = false;
var pageIsList = true;
var pageType = "";
var pageEditable = false;
var uipath = '/teamplayer/ui';
var highlightListEntries = {};
var activeList = '';
var reloadOnEdit = true;
var doAutoSave = false;
var doLockChecks = true;
var internalApp = true;
var interceptKeys = true;
var trySubmitTimer;

function setLogoutLink() {
	try {
		var tmp = window.top.header.$("logoutButtonAnchor");
	} catch(e) {
		window.location.assign("/teamplayer/index.php");
		return;
	}
	if (!top.header.$("logoutButton")) {
		var tmpText = "<div id='logoutButton' class='logoutButton' title='logged in as Joe Murphy' onclick=\"top.window.location.assign('/teamplayer/ui/logout.php');\" onmouseover=\"$(this).addClassName('logoutButtonHover');\" onmouseout=\"$(this).removeClassName('logoutButtonHover');\">Log out</div>";
		top.header.$("logoutButtonAnchor").update(tmpText);
	}
}

function init() {
	var startRow = false;
	if (pageIsList) {
		setLogoutLink();
		if (document.all) {
			document.onkeydown=processKeys;
		} else {
			document.onkeypress=processKeys;
		}
		var filterOptions = $("listTableLeft").select('.filterOption');
		var observer = function(){interceptKeys=false;};
		for (i = 0; i < filterOptions.length; i++) {
			addEventToElement(filterOptions[i], "focus", observer, false) 
		}
		interceptKeys = true;
		if ($("row" + highlightListEntries[activeList])) {
			startRow = $("row" + highlightListEntries[activeList]);
		}
	}
	if (pageEditable) {
		initializeFocus(pageType);
		if (pageType == "entry" || pageType == "formEdit") {
			$("entryCurtain").hide();
		}
		if (pageType == "" || pageType == "formEdit") {
			setDefaults();
		}
				if (startInEditMode) {
			switchMode(true);
		 
		} else {
		switchMode(false);
		}
				if ($("buttonPanelSet1")) {
			$("buttonPanelSet1").show();
		}
		if ($("buttonPanelSet2")) {
			$("buttonPanelSet2").show();
		}
			}
	resizeWindow();
	if (startRow) {
		moveTo(startRow, 0, true, false);
	}
}
</script>
</head>
<body onload="init();" ondblclick="doubleClickForm();" onresize="resizeWindow();" onunload="basicClose();">
<script language="javascript" type="text/javascript">

var listboxActiveItem = -1;
var keyScrolling = false;
var elemTop = 0;
var elemHeight = 0;
var elemBottom = 0;
var scrollerTop = 0;
var scrollerHeight = 0;
var scrollerBottom = 0;

var doRead;
var doEdit;
var doClone;

var instrux = {
	tabStats: "Enter game stats here",
	tabMeetStats: "Enter meet stats here",
	tabPlayers: "Set up players and team rosters",
	tabUser: 'Manage accounts for internal users',
	tabWeb: "Set up templates for web content display",
	tabPrint: "Set up templates and deliveries for print exports",
	tabSeason: "Set up schedules, sports and other season items",
	tabSetup: "Set up stat entry forms, stat rules and users",
	tabGame: "Review and edit schedules for the season"
	}

var tabReloadArgs = {
	tabStats: "type=Game&stats=1",
	tabMeetStats: "type=Meet&stats=1",
	tabSeason: "type=Game",
	tabSetup: "type=Form",
	tabPlayers: "type=Player",
	tabWeb: "type=WebTemplate",
	tabPrint: "type=Delivery"
	}
	
function showSubTabs(mainTab) {
	window.location = "/teamplayer/ui/list.php?" + tabReloadArgs[mainTab.id];
}	

function loadList(type) {
	window.location = "/teamplayer/ui/list.php?type=" + type;
}

function showTabHelp(element) {
	$("tabHelp").update(instrux[element.id]).show();
	if ($(element).hasClassName("tabMain")) {
		$(element).addClassName("tabMainHover");
	}
	if ($(element).hasClassName("tabSub")) {
		$(element).addClassName("tabSubHover");
	}
}

function clearTabHelp(element) {
	$("tabHelp").update("").hide();
	if ($(element).hasClassName("tabMain")) {
		$(element).removeClassName("tabMainHover");
	}
	if ($(element).hasClassName("tabSub")) {
		$(element).removeClassName("tabSubHover");
	}
}

function setGlobalTabActions(className) {
	var tmpArray = $$('.'+className);
	tmpArray.each(function(elem) {
		elem.onmouseover = function(){showTabHelp(this);};
		elem.onmouseout = function(){clearTabHelp(this);};
		if (className == "tabMain") {
			if (elem.onclick == undefined) {
				elem.onclick = function(){showSubTabs(this);};
			}
		} else if (className == "tabSub") {
			if (elem.onclick == undefined) {
				elem.onclick = function(){loadList(this.id.substring(3));};
			}
		}
	});
}
</script>

