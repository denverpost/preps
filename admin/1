<div id="cpDate" style="display:none;" title="Click the icon to display calendar, arrow keys and &lt;enter&gt; to choose date, &lt;esc&gt; to close">
<b>date:</b><br />
<img src="/teamplayer/ui/graphics/calendar.png" class="icon button" alt="Pick date." id="FromDateCal">
<input type="text" size="10" name="FromDate" id="FromDate" onkeydown="return checkCalKeys(event, this)" onkeyup="fetchSearch(this.value,event)" class="field" value="11/27/2007" title = "t for tomorrow, d for today, or first letters of days of the week: su, m, tu, w, th, f, sa"  onchange="submitSearch()" >
<script language="javascript" type="text/javascript">
Calendar.setup({
	inputField:"FromDate",
	ifFormat:"%m/%d/%Y",
	button:"FromDateCal",
	singleClick:true,
	onSelect:selectDate,
	weekNumbers:false
	});
</script>

<label for="ToDate" class="filterLabel">To</label>
<img src="/teamplayer/ui/graphics/calendar.png" class="icon button" alt="Pick date." id="ToDateCal">
<input type="text" size="10" name="ToDate" id="ToDate" onkeydown="return checkCalKeys(event, this)" onkeyup="fetchSearch(this.value,event)" class="field" value="12/07/2007" title = "t for tomorrow, d for today, or first letters of days of the week: su, m, tu, w, th, f, sa" onchange="submitSearch()" >
<script language="javascript" type="text/javascript">
Calendar.setup({
	inputField:"ToDate",
	ifFormat:"%m/%d/%Y",
	button:"ToDateCal",
	singleClick:true,
	onSelect:selectDate,
	weekNumbers:false
	});
</script>
